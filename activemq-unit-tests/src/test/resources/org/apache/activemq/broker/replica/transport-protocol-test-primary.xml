        <!-- START SNIPPET: example -->
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:amq="http://activemq.apache.org/schema/core"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
  http://activemq.apache.org/schema/core http://activemq.apache.org/schema/core/activemq-core.xsd">

    <broker useJmx="false" persistent="false" xmlns="http://activemq.apache.org/schema/core" populateJMSXUserID="true">

        <destinations>
            <queue physicalName="TEST.Q" />
        </destinations>

        <transportConnectors>
            <transportConnector name="vm" uri="vm://firstbroker" />
            <transportConnector name="mqtt" uri="mqtt://0.0.0.0:0"/>
            <transportConnector name="auto" uri="auto://0.0.0.0:0"/>
            <transportConnector name="stomp" uri="stomp://0.0.0.0:0"/>
            <transportConnector name="tcp" uri="tcp://0.0.0.0:0"/>
            <transportConnector name="mqtt+nio" uri="mqtt+nio://0.0.0.0:0"/>
            <transportConnector name="auto+nio" uri="auto+nio://0.0.0.0:0"/>
            <transportConnector name="stomp+nio" uri="stomp+nio://0.0.0.0:0"/>
            <transportConnector name="nio" uri="nio://0.0.0.0:0"/>
            <transportConnector name="nio+ssl" uri="nio+ssl://0.0.0.0:0"/>
            <transportConnector name="mqtt+nio+ssl" uri="mqtt+nio+ssl://0.0.0.0:0"/>
            <transportConnector name="auto+nio+ssl" uri="auto+nio+ssl://0.0.0.0:0"/>
            <transportConnector name="stomp+nio+ssl" uri="stomp+nio+ssl://0.0.0.0:0"/>
            <transportConnector name="mqtt+ssl" uri="mqtt+ssl://0.0.0.0:0"/>
            <transportConnector name="auto+ssl" uri="auto+ssl://0.0.0.0:0"/>
            <transportConnector name="stomp+ssl" uri="stomp+ssl://0.0.0.0:0"/>
            <transportConnector name="ssl" uri="ssl://0.0.0.0:0"/>
        </transportConnectors>

        <plugins>
            <replicaPlugin role="source" transportConnectorUri="tcp://0.0.0.0:61618" otherBrokerUri="tcp://localhost:61619" userName="replicaUser" password="replica"/>
        </plugins>
    </broker>

</beans>
